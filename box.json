{
    "alias": "jupyter-php-installer.phar",
    "chmod": "0755",
    "directories": ["src"],
    "files": [
        "LICENSE",
        "README.md",
        "public.key"
    ],
    "finder": [
        {
            "name": "*.php",
            "exclude": ["Tests", "kherge"],
            "in": "vendor"
        }
    ],
    "git-version": "package_version",
    "main": "src/EntryPoint/main.php",
    "output": "jupyter-php-installer.phar",
    "stub": true
}